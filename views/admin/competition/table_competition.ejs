          <!-- DataTales Example -->
          <div class="card shadow mb-4">
            <div class="card-header py-3">
              <a href="/admin/competitions/add" class="btn btn-success">Add Competition</a>
            </div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                  <thead>
                    <tr>
                      <th>Title</th>
                      <th>Deadline</th>
                      <th>Company</th>
                      <th>Image</th>
                      <th>Action</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% for(let i = 0; i < competitions.length; i++) { %>
                      <tr>
                        <td><%= competitions[i].title %></td>
                        <td><%= competitions[i].deadline %></td>
                        <td><%= competitions[i].company %></td>
                        <td><img src="<%= competitions[i].imageUrl %>" alt="imageUrl" width="70px" height="100%" /></td>
                        <td>
                          <form action="/admin/competitions/delete/<%= competitions[i].id %>?_method=DELETE" method="POST" >
                            <button type="submit" class="btn btn-danger btn-sm"><i class="fas fa-trash"></i></button>
                          </form>
                        </td>
                      </tr>
                    <% } %>
                  </tbody>

                </table>
              </div>
            </div>
          </div>