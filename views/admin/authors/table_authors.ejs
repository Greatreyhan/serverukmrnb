          <!-- DataTales Example -->
          <div class="card shadow mb-4">
            <div class="card-header py-3">
              <a href="/admin/authors/add" class="btn btn-success">Add Authors</a>
            </div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                  <thead>
                    <tr>
                      <th>Name</th>
                      <th>Image</th>
                      <th>Occupation</th>
                      <th>City</th>
                      <th>Action</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% for(let i = 0; i < authors.length; i++){  %>
                      <tr>
                        <td><%= authors[i].name %></td>
                        <td><img src="<%= authors[i].imageUrl %>" alt="imageUrl" width="70px" height="100%" /></td>
                        <td><%= authors[i].occupation %></td>
                        <td><%= authors[i].city %></td>
                        <td>
                          <form action="/admin/authors/delete/<%= authors[i].id %>?_method=DELETE" method="POST" >
                            <a href="/admin/authors/edit/<%= authors[i].id %>" class="btn btn-warning btn-sm" ><i class="fas fa-edit"></i></a>
                            <button type="submit" class="btn btn-danger btn-sm"><i class="fas fa-trash"></i></button>
                          </form>
                        </td>
                      </tr>
                    <% } %>
                    <tr></tr>
                  </tbody>

                </table>
              </div>
            </div>
          </div>