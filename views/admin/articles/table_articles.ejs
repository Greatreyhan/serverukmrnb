          <!-- DataTales Example -->
          <div class="card shadow mb-4">
            <div class="card-header py-3">
              <a href="/admin/articles/add" class="btn btn-success">Add Article</a>
            </div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                  <thead>
                    <tr>
                      <th>Title</th>
                      <th>Date</th>
                      <th>Reader</th>
                      <th>Image</th>
                      <th>Action</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% for(let i = 0; i < articles.length; i++) { %>
                      <tr>
                        <td><%= articles[i].title %></td>
                        <td><%= articles[i].date %></td>
                        <td><%= articles[i].reader %></td>
                        <td><img src="<%= articles[i].imageUrl %>" alt="imageUrl" width="70px" height="100%" /></td>
                        <td>
                          <form action="/admin/articles/delete/<%= articles[i].id %>?_method=DELETE" method="POST" >
                            <a href="/admin/articles/edit/<%= articles[i].id %>" class="btn btn-warning btn-sm" ><i class="fas fa-edit"></i></a>
                            <a href="/admin/articles/detail/<%= articles[i].id %>" class="btn btn-success btn-sm" ><i class="fas fa-eye"></i></a>
                            <button type="submit" class="btn btn-danger btn-sm"><i class="fas fa-trash"></i></button>
                          </form>
                        </td>
                      </tr>
                    <% } %>
                  </tbody>

                </table>
              </div>
            </div>
          </div>