          <!-- DataTales Example -->
          <div class="card shadow mb-4">
            <div class="card-header py-3">
              <a href="/admin/tags/add" class="btn btn-success">Add Tag</a>
            </div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                  <thead>
                    <tr>
                      <th>Name</th>
                      <th>Action</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% for(let i = 0; i < tags.length; i++){ %>
                      <tr>
                        <td><%= tags[i].name %></td>
                        <td>
                          <form action="/admin/tags/delete/<%= tags[i].id %>?_method=DELETE" method="POST" >
                            <a href="/admin/tags/edit/<%= tags[i].id %>" class="btn btn-warning btn-sm" ><i class="fas fa-edit"></i></a>
                            <button type="submit" class="btn btn-danger btn-sm"><i class="fas fa-trash"></i></button>
                          </form>
                        </td>
                      </tr>
                    <% } %>
                  </tbody>

                </table>
              </div>
            </div>
          </div>